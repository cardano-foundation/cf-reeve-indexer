{
  "name": "reeve-public-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "reeve-platform-version": "release/1.2.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "fix": "npx eslint --fix 'src/**/*.{ts,tsx}'",
    "ts": "tsc",
    "format": "npx prettier . --write",
    "test:localhost": "npx cross-env NODE_OPTIONS=\"--loader=ts-node/esm --no-warnings\" bddgen && npx playwright test --project=e2e-tests --workers=1",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "@mui/icons-material": "^7.0.0",
    "@mui/material": "^7.0.0",
    "@mui/x-charts": "^8.0.0",
    "@mui/x-data-grid": "^7.0.0",
    "@mui/x-date-pickers": "^7.0.0",
    "@tanstack/react-query": "^5.0.0",
    "axios": "^1.0.0",
    "dayjs": "^1.0.0",
    "dotenv": "^16.0.0",
    "formik": "^2.0.0",
    "html-react-parser": "^5.0.0",
    "i18n-iso-countries": "^7.0.0",
    "iconsax-react": "^0.0.8",
    "lodash": "^4.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-idle-timer": "^5.0.0",
    "react-intl": "^6.0.0",
    "react-router-dom": "^6.0.0",
    "styled-components": "^6.0.0",
    "yup": "^1.0.0"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.0.0",
    "@tanstack/react-query-devtools": "^5.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^15.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/lodash": "^4.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^6.0.0",
    "eslint-plugin-material-ui": "^1.0.0",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "eslint-plugin-react-refresh": "^0.4.0",
    "husky": "^9.0.0",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.0.0",
    "msw": "^2.0.0",
    "prettier": "^3.0.0",
    "sass": "^1.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0",
    "vite": "^7.1.5",
    "vite-tsconfig-paths": "^4.0.0",
    "vitest": "^3.2.4"
  }
}
